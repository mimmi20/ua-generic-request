{
  "name": "mimmi20/ua-generic-request",
  "description": "the Generic Request Object extracted from Wurfl for PHP 7.1+",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/ua-generic-request/graphs/contributors"
    }
  ],
  "homepage": "https://github.com/mimmi20/ua-generic-request",
  "require": {
    "php": "~8.3.0 || ~8.4.0 || ~8.5.0",
    "ext-mbstring": "*",
    "laminas/laminas-diactoros": "^3.5.0",
    "mimmi20/ua-loader-interface": "^4.0.1",
    "mimmi20/ua-normalizer": "^7.0.0",
    "mimmi20/ua-parser-interface": "^2.0.0",
    "psr/http-message": "^1.1 || ^2.0"
  },
  "require-dev": {
    "ext-ctype": "*",
    "ext-dom": "*",
    "ext-simplexml": "*",
    "ext-tokenizer": "*",
    "ext-xml": "*",
    "ext-xmlwriter": "*",
    "infection/infection": "^0.29.14",
    "mimmi20/browser-detector-version": "^11.0.0",
    "mimmi20/coding-standard": "^6.0.27",
    "mimmi20/ua-browser-type": "^11.0.0",
    "mimmi20/ua-result": "^12.0.0",
    "nikic/php-parser": "^5.4.0",
    "phpstan/extension-installer": "^1.4.3",
    "phpstan/phpstan": "^2.1.12",
    "phpstan/phpstan-deprecation-rules": "^2.0.1",
    "phpunit/phpunit": "^12.1.2",
    "rector/rector": "^2.0.11",
    "rector/type-perfect": "^2.0.2",
    "shipmonk/composer-dependency-analyser": "^1.8.2",
    "symfony/process": "^7.2.5",
    "symplify/phpstan-rules": "^14.6.3",
    "tomasvotruba/cognitive-complexity": "^1.0.0",
    "tomasvotruba/type-coverage": "^2.0.2",
    "tomasvotruba/unused-public": "^2.0.1"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "UaRequest\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "UaRequestTest\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "infection/extension-installer": true,
      "phpstan/extension-installer": true
    },
    "lock": false,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
