{
  "name": "mimmi20/ua-generic-request",
  "type": "library",
  "description": "the Generic Request Object extracted from Wurfl for PHP 7.1+",
  "homepage": "https://github.com/mimmi20/ua-generic-request",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/ua-generic-request/graphs/contributors"
    }
  ],
  "require": {
    "php": "^7.1.3 || ^8.0.0",
    "laminas/laminas-diactoros": "^2.3.1",
    "mimmi20/browser-detector-loader-interface": "~3.0.7",
    "psr/http-message": "^1.0.1"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.6.1",
    "ergebnis/phpstan-rules": "^0.15.1",
    "infection/infection": "^0.13.6 || ^0.16.4",
    "mimmi20/coding-standard": "~1.0.1",
    "mimmi20/json-class": "~2.0.11",
    "pepakriz/phpstan-exception-rules": "^0.11.1",
    "phpstan/extension-installer": "^1.0.4",
    "phpstan/phpstan": "^0.12.35",
    "phpstan/phpstan-deprecation-rules": "^0.12.5",
    "phpstan/phpstan-phpunit": "^0.12.15",
    "phpstan/phpstan-strict-rules": "^0.12.4",
    "phpunit/phpunit": "^7.5.20 || ^8.5.8 || ^9.2.6",
    "symfony/finder": "^4.4.11 || ^5.1.3"
  },
  "config": {
    "platform": {
      "php": "7.1.3"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "UaRequest\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "UaRequestTest\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
