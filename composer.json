{
  "name": "mimmi20/ua-generic-request",
  "type": "library",
  "description": "the Generic Request Object extracted from Wurfl for PHP 7.1+",
  "homepage": "https://github.com/mimmi20/ua-generic-request",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/ua-generic-request/graphs/contributors"
    }
  ],
  "require": {
    "php": "^7.1.3 || ^8.0.0",
    "laminas/laminas-diactoros": "~2.4.1 || ~2.5.0",
    "mimmi20/browser-detector-loader-interface": "~3.0.13",
    "psr/http-message": "~1.0.1"
  },
  "require-dev": {
    "infection/infection": "~0.13.6 || ~0.18 || ~0.20.2",
    "mimmi20/coding-standard": "~1.0.9",
    "mimmi20/json-class": "~2.0.17",
    "pepakriz/phpstan-exception-rules": "~0.11.5",
    "phpstan/extension-installer": "~1.1.0",
    "phpstan/phpstan": "~0.12.70",
    "phpstan/phpstan-deprecation-rules": "~0.12.6",
    "phpstan/phpstan-phpunit": "~0.12.17",
    "phpunit/phpunit": "~7.5.20 || ~8.5.13 || ~9.5.0",
    "symfony/finder": "~4.4.18 || ~5.2.1"
  },
  "config": {
    "platform": {
      "php": "7.1.3"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "UaRequest\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "UaRequestTest\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
