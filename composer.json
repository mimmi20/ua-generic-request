{
  "name": "mimmi20/ua-generic-request",
  "description": "the Generic Request Object extracted from Wurfl for PHP 7.1+",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/ua-generic-request/graphs/contributors"
    }
  ],
  "homepage": "https://github.com/mimmi20/ua-generic-request",
  "require": {
    "php": "^8.1.0",
    "ext-mbstring": "*",
    "laminas/laminas-diactoros": "^3.2.0",
    "psr/http-message": "^1.1 || ^2.0"
  },
  "require-dev": {
    "ext-ctype": "*",
    "ext-dom": "*",
    "ext-simplexml": "*",
    "ext-tokenizer": "*",
    "ext-xml": "*",
    "ext-xmlwriter": "*",
    "infection/infection": "^0.27.0",
    "mimmi20/coding-standard": "^5.1.13",
    "nikic/php-parser": "^4.17.1",
    "phpstan/extension-installer": "^1.3.1",
    "phpstan/phpstan": "^1.10.33",
    "phpstan/phpstan-deprecation-rules": "^1.1.4",
    "phpstan/phpstan-phpunit": "^1.3.14",
    "phpunit/phpunit": "^10.3.3",
    "rector/rector": "^0.18.1",
    "vimeo/psalm": "^5.15.0"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "UaRequest\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "UaRequestTest\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "phpstan/extension-installer": true,
      "infection/extension-installer": true
    }
  }
}
