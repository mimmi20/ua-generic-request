{
  "name": "mimmi20/ua-generic-request",
  "type": "library",
  "description": "the Generic Request Object extracted from Wurfl for PHP 7.1+",
  "keywords": [
    "generic request"
  ],
  "homepage": "https://github.com/mimmi20/ua-generic-request",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/ua-generic-request/graphs/contributors"
    }
  ],
  "require": {
    "php": "^7.1",
    "laminas/laminas-diactoros": "^2.2.2",
    "mimmi20/browser-detector-loader-interface": "~3.0.3",
    "psr/http-message": "^1.0.1"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.2.2",
    "ergebnis/phpstan-rules": "^0.14.2",
    "friendsofphp/php-cs-fixer": "^2.16.1",
    "infection/infection": "^0.13.6",
    "mimmi20/json-class": "~2.0.8",
    "pepakriz/phpstan-exception-rules": "^0.10.1",
    "phpstan/phpstan": "^0.12.5",
    "phpstan/phpstan-deprecation-rules": "^0.12.2",
    "phpstan/phpstan-phpunit": "^0.12.6",
    "phpstan/phpstan-strict-rules": "^0.12.1",
    "phpunit/phpunit": "^7.5.19 || ^8.5.0",
    "squizlabs/php_codesniffer": "^3.5.3",
    "symfony/finder": "^4.4.2 || 5.0.1"
  },
  "config": {
    "platform": {
      "php": "7.1.3"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "UaRequest\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "UaRequestTest\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
